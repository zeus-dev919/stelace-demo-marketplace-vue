

{
	"masterPicture": "TODO: Path to master picture",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#ffffff",
			"margin": "14%",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			},
			"appName": "Stelace"
		},
		"desktopBrowser": {},
		"windows": {
			"masterPicture": {
				"type": "inline",
				"content": ""
			},
			"pictureAspect": "noChange",
			"backgroundColor": "#b91d47",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": true,
					"medium": true,
					"big": true,
					"rectangle": false
				}
			},
			"appName": "Stelace"
		},
		"androidChrome": {
			"pictureAspect": "shadow",
			"themeColor": "#ffffff",
			"manifest": {
				"name": "Stelace",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "blackAndWhite",
			"threshold": 64.0625,
			"themeColor": "#1055b6"
		}
	},
	"settings": {
		"compression": 1,
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": true,
		"usePathAsIs": false
	}
